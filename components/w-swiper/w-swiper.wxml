<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper"
        circular
        autoplay="{{flg}}"
        interval="4000"
        indicator-dots
        indicator-active-color="#ff5777"
        duration="1000">
<block wx:for="{{lists}}" wx:key="{{index}}">
<swiper-item >
<image class="swiper-item" src="{{item.image || item}}" mode="widthFix" />
</swiper-item>
</block>
</swiper>
