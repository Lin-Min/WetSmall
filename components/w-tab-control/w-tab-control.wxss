/* components/w-tab-control/w-tab-control.wxss */
.tab-control {
  display: flex;
  margin-bottom: 30rpx;
  background: #fff;
}

.tab-item {
  flex: 1;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  color: #aca9a9;
}
.tab-item text{
  font-size:32rpx; 
}
.active {
  color: #ff5777;
}

.tab-item.active text {
  padding: 16rpx 10rpx;
  border-bottom: 6rpx solid #ff5777;
}
